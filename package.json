{
  "name": "@balena/jellyfish-plugin-balena-api",
  "description": "Balena API Jellyfish Plugin",
  "version": "6.0.276",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-balena-api"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ],
    "*.{json,yml}": [
      "prettier -w"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint && prettier --check **/*.json **/*.yml",
    "lint:fix": "balena-lint --fix lib test && prettier -w **/*.json **/*.yml",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "npx jest --runInBand --bail test/integration",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.87",
    "@balena/jellyfish-environment": "^14.3.13",
    "@balena/jellyfish-logger": "^5.1.121",
    "@balena/jellyfish-worker": "^35.0.53",
    "axios": "^0.27.2",
    "geoip-lite": "^1.4.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-jose": "^2.1.1",
    "randomstring": "^1.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.0",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.186",
    "@types/node-jose": "^1.1.10",
    "@types/randomstring": "^1.1.8",
    "@types/uuid": "^8.3.4",
    "autumndb": "^22.2.146",
    "deplint": "^1.1.3",
    "jest": "^29.2.0",
    "lint-staged": "^13.0.3",
    "native-url": "^0.3.4",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.16",
    "typescript": "^4.8.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-10-15T09:04:36.548Z"
  }
}
