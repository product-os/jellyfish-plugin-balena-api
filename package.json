{
  "name": "@balena/jellyfish-plugin-balena-api",
  "description": "Balena API Jellyfish Plugin",
  "version": "6.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-balena-api"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ],
    "*.{json,yml}": [
      "prettier -w"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint && prettier --check **/*.json **/*.yml",
    "lint:fix": "balena-lint --fix lib test && prettier -w **/*.json **/*.yml",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "npx jest --runInBand --bail test/integration",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.106",
    "@balena/jellyfish-environment": "^15.0.0",
    "@balena/jellyfish-logger": "^5.1.126",
    "@balena/jellyfish-worker": "^36.0.00",
    "axios": "^1.0.0",
    "geoip-lite": "^1.4.6",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "node-jose": "^2.1.1",
    "randomstring": "^1.2.3"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.1",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.190",
    "@types/node": "^18.11.18",
    "@types/node-jose": "^1.1.10",
    "@types/randomstring": "^1.1.8",
    "autumndb": "^22.2.186",
    "deplint": "^1.1.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.4",
    "native-url": "^0.3.4",
    "nock": "^13.2.9",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2023-01-05T15:48:16.436Z"
  }
}
