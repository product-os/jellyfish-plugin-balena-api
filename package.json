{
  "name": "@balena/jellyfish-plugin-balena-api",
  "description": "Balena API Jellyfish Plugin",
  "version": "6.8.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-balena-api"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ],
    "*.{json,yml}": [
      "prettier -w"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint && prettier --check **/*.json **/*.yml",
    "lint:fix": "balena-lint --fix lib test && prettier -w **/*.json **/*.yml",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "npx jest --runInBand --forceExit test/integration",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.11.2",
    "@balena/jellyfish-environment": "^21.0.0",
    "@balena/jellyfish-logger": "^6.0.1",
    "@balena/jellyfish-worker": "^47.0.1",
    "axios": "^0.27.2",
    "geoip-lite": "^1.4.7",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "node-jose": "^2.2.0",
    "randomstring": "^1.2.3"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.2",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/node-jose": "^1.1.10",
    "@types/randomstring": "^1.1.8",
    "autumndb": "^26.0.2",
    "deplint": "^1.1.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "native-url": "^0.3.4",
    "nock": "^13.3.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "simple-git-hooks": "^2.8.1",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2023-04-04T07:46:12.900Z"
  }
}
