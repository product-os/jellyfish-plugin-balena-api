{
  "name": "@balena/jellyfish-plugin-balena-api",
  "description": "Balena API Jellyfish Plugin",
  "version": "1.0.115",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-balena-api"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "npx jest --runInBand --bail test/integration",
    "test:compose": "DOCKER_BUILDKIT=1 docker build -t balena/jellyfish-sut:latest --secret id=npmrc,src=.balena/secrets/npmrc.txt . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.68",
    "@balena/jellyfish-environment": "^5.0.10",
    "@balena/jellyfish-logger": "^3.0.96",
    "@balena/jellyfish-plugin-base": "^2.1.255",
    "axios": "^0.23.0",
    "bluebird": "^3.7.2",
    "geoip-lite": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-jose": "^2.0.0",
    "randomstring": "^1.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.5",
    "@balena/jellyfish-action-library": "^15.1.2",
    "@balena/jellyfish-config": "^1.5.1",
    "@balena/jellyfish-core": "^7.0.2",
    "@balena/jellyfish-plugin-default": "^21.1.19",
    "@balena/jellyfish-sync": "^6.1.3",
    "@balena/jellyfish-test-harness": "^8.1.9",
    "@balena/jellyfish-types": "^1.2.1",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
    "@types/node-jose": "^1.1.8",
    "@types/randomstring": "^1.1.7",
    "@types/uuid": "^8.3.1",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.4",
    "native-url": "^0.3.4",
    "nock": "^13.1.4",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.6.1",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.6",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@balena/jellyfish-action-library": "^15.1.2",
    "@balena/jellyfish-core": "^7.0.2",
    "@balena/jellyfish-plugin-default": "^21.1.19",
    "@balena/jellyfish-sync": "^6.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-10-28T11:47:27.601Z"
  }
