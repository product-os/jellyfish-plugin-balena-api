    "randomstring": "^1.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-action-library": "^15.1.189",
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-core": "^8.2.2",
    "@balena/jellyfish-plugin-default": "^21.1.147",
    "@balena/jellyfish-sync": "^6.1.130",
    "@balena/jellyfish-test-harness": "^8.1.204",
    "@balena/jellyfish-types": "^1.2.39",
    "@balena/lint": "^6.2.0",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^27.0.3",
