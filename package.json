{
  "name": "@balena/jellyfish-plugin-balena-api",
  "description": "Balena API Jellyfish Plugin",
  "version": "2.0.55",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-balena-api"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "npx jest --runInBand --bail test/integration",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.18",
    "@balena/jellyfish-environment": "^9.1.4",
    "@balena/jellyfish-logger": "^5.0.6",
    "@balena/jellyfish-worker": "^20.0.4",
    "axios": "^0.26.1",
    "geoip-lite": "^1.4.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-jose": "^2.1.0",
    "randomstring": "^1.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.3",
    "@balena/jellyfish-plugin-default": "^24.0.22",
    "@balena/jellyfish-plugin-product-os": "^4.0.52",
    "@balena/jellyfish-types": "^2.0.4",
    "@balena/lint": "^6.2.0",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.179",
    "@types/node-jose": "^1.1.9",
    "@types/randomstring": "^1.1.8",
    "@types/uuid": "^8.3.4",
    "deplint": "^1.1.3",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "native-url": "^0.3.4",
    "nock": "^13.2.4",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-03-16T18:51:16.953Z"
  }
}
